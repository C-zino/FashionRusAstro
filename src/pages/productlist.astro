---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header.astro";
import ProductCard from "../components/ProductCard.astro";
const data = await fetch("https://kea-alt-del.dk/t7/api/products").then((res) =>
  res.json()
);
console.log(data);
---

<Layout />
<Header />
<main>
  <h2>Produktliste</h2>
  <div class="productlist">
    {data.map((element) => <ProductCard data={element} />)}
  </div>
</main>
